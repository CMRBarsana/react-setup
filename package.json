{
  "name": "vite-setup",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint --fix",
    "watch:style": "postcss -w src/assets/tailwind/styles.css -o src/assets/styles/styles.css",
    "build:style": "postcss src/assets/tailwind/styles.css -o src/assets/styles/styles.css",
    "vite:start": "vite",
    "dev": "npm-run-all -p vite:start watch:style"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "@types/lodash": "^4.14.202",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^16.0.0",
    "postcss-minify": "^1.1.0",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.4",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.34",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-eslint": "^1.8.1"
  }
}
